{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ESNext",
    "module": "preserve",
    "lib": [
      "ESNext"
    ],
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ]
}